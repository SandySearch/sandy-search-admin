    "handlebars": "^4.7.7",
    "hoek": "^4.2.1",
    "https-proxy-agent": "^2.2.3",
    "js-yaml": "^3.13.1",
    "lodash.template": "^4.5.0",
    "randomatic": "^3.0.0",
    "set-value": "^4.0.1",
