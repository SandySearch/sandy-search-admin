{
  "name": "sandysearch-admin",
  "version": "1.0.5",
  "repository": "git+https://github.com/sandysearch/sandy-search-admin",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "deploy": "ng build --base-href /sandy-search-admin/ && npx ngh --dir=dist --no-silent",
    "do-not-use-deploy": "ng build --prod --base-href /sandy-search-admin/ && npx ngh --dir=dist --no-silent",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "do-not-usefbserve": "npm run-script build && firebase serve",
    "do-not-usefbdeploy": "npm run-script build && firebase deploy",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.1",
    "@angular/common": "^2.3.1",
